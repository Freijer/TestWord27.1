<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="4dip" android:color="#02193E" />
            <solid android:color="#93B4E8"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="40dip" />
            <stroke android:width="4dip" android:color="#050505" />
            <solid android:color="#7BA5E8"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle"  >
            <corners android:radius="60dip" />
            <stroke android:width="3dip" android:color="#000000" />
            <solid android:color="#929EEF"/>
        </shape>
    </item>
</selector>